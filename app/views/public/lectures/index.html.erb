<div class="container text-center">
  <div class="py-5 px-4 px-md-5">
    <h2 class="fw-bolder mb-5">教室一覧</h2>
    <% @lectures.each do |lecture| %>
      <div class="row justify-content-center">
        <div class="col">
        <h3 class="fw-bolder ">
        <%= link_to lecture.lecture_name, lecture_path(lecture.id) %>
        </h3>
        </div>
      </div>
    <% end %>
    <%= paginate @lectures %>
  </div>
</div>
